<!-- When the addon is installed, OSB EXTENSION NAME delimeter will be replaced by your addon name -->
<div id="OSB-theme-app-extension-OSB_EXTENSION_NAME"></div>

{% schema %}
  {
    "name": "OSB Theme Extension Base",
    "target": "section",
    "stylesheet": "app.css",
    "javascript": "app.js",
    "settings": [
        { "label": "Color", "id": "color", "type": "color", "default": "#000000" }
    ]
  }
{% endschema %}